<config>
	<browser>firefox "%b"</browser>

	<proxy-address></proxy-address>
	<proxy-port>0</proxy-port>
	<proxy-type>HTTP</proxy-type>

	<timeout>90</timeout>
	<maxthreads>20</maxthreads>

	<category name="Ebook">
		<engine version="1.0" path="xml/aleria.xml"/>
		<engine version="1.0" path="xml/ebooksportal.xml"/>
		<engine version="1.0" path="xml/archive.xml"/>
		<engine version="1.0" path="xml/lookforbook.xml"/>
	</category>
	<category name="English">
		<engine version="1.0" path="xml/alltheweb.xml"/>
		<engine version="1.0" path="xml/altavista.xml"/>
		<engine version="1.0" path="xml/ask.xml"/>
		<engine version="1.0" path="xml/dmoz.xml"/>
		<engine version="1.0" path="xml/dogpile.xml"/>
		<engine version="1.0" path="xml/google.xml"/>
		<engine version="1.0" path="xml/livesearch.xml"/>
		<engine version="1.0" path="xml/yahoo.xml"/>
		<engine version="1.0" path="xml/exalead.xml" />
	</category>
	<category name="Files">
		<engine version="1.0" path="xml/filesearching.xml"/>
		<engine version="1.0" path="xml/filewatcher.xml"/>
		<engine version="1.0" path="xml/findfiles.xml"/>
		<engine version="1.0" path="xml/ftpindexer.xml"/>
		<engine version="1.0" path="xml/googlefiles.xml"/>
		<engine version="1.0" path="xml/metaftp.xml"/>
		<engine version="1.0" path="xml/napalm.xml"/>
		<engine version="1.0" path="xml/ramblerftp.xml"/>
		<engine version="1.0" path="xml/yahoofiles.xml"/>
	</category>
	<category name="French">
		<engine version="1.0" path="xml/aefdmoz.xml"/>
		<engine version="1.0" path="xml/allthewebfr.xml"/>
		<engine version="1.0" path="xml/altavistafr.xml"/>
		<engine version="1.0" path="xml/askfr.xml"/>
		<engine version="1.0" path="xml/googlefr.xml"/>
		<engine version="1.0" path="xml/livesearchfr.xml"/>
		<engine version="1.0" path="xml/voilafr.xml"/>
		<engine version="1.0" path="xml/yahoofr.xml"/>
	</category>
	<category name="Images">
		<engine version="1.0" path="xml/dogpileimg.xml"/>
		<engine version="1.0" path="xml/yahooimg.xml"/>
	</category>
	<category name="IRC">
		<engine version="1.0" path="xml/packetnews.xml"/>
	</category>
	<category name="News French">
		<engine version="1.0" path="xml/easybourse-forums.xml"/>
	</category>
	<category name="Research">
		<engine version="1.0" path="xml/arxiv.xml"/>
		<engine version="1.0" path="xml/citebase.xml"/>
		<engine version="1.0" path="xml/citeseer.xml"/>
	</category>
	<category name="Torrent">
		<engine version="1.0" path="xml/isohunt.xml"/>
	</category>
	<category name="Videos">
		<engine version="1.0" path="xml/aolvideo.xml"/>
		<engine version="1.0" path="xml/blinkx.xml"/>
		<engine version="1.0" path="xml/searchforvideo.xml"/>
		<engine version="1.0" path="xml/yahoovideo.xml"/>
		<engine version="1.0" path="xml/youtube.xml"/>
	</category>
</config>
